{
  "$schema": "../../../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Dandelion",
  "version": "0.1.0",
  "identifier": "cn.acitrus.dev",
  "build": {
    "frontendDist": "../../../dist",
    "devUrl": "http://localhost:9527/",
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build"
  },
  "app": {
    "windows": [
      {
        "title": "Dandelion",
        "center": true,
        "width": 1600,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "decorations": true,
        "hiddenTitle": true,
        "titleBarStyle": "Transparent",
        "transparent": true,
        "devtools": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
